    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-sonarjs": "0.5.0",
    "jest": "26.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5"
